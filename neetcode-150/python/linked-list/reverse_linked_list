# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def reverseList(self, head):
        pass

    def reverseListRec(self, head):
        if head.next == None:
            return head
        point = self.reverseListRec(head)
        point.next = head
        return point


solution = Solution()
